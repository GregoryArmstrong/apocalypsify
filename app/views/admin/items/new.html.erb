<h1> Create Item </h2>

<%= form_for @item, url: admin_items_path do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :description %>
  <%= f.text_field :description %>
  <%= f.label :image_url, "Image URL"  %>
  <%= f.text_field :image_url %>
  <%= f.label :price %>
  <%= f.text_field :price %>
  <%= f.label "Travesty" %>
  <%= f.collection_select :travesty_id, Travesty.all.distinct, :id, :title, include_blank: true %>

  <%= f.submit "Create Item" %>
<% end %>
