      <div class="sign-up container">
        <div class="col s12 m5">
          <div class="card-panel">
            <h3 class="center"> Sign In: </h3>
              <%= simple_form_for :session, url: login_path do |f| %>

              <%= f.input :username %>
              <br>
              <%= f.input :password %>
              <br>
              <div class="center", id="new_user_submit">
                <a class="waves-effect waves-light btn"><%= f.submit "Sign In"%></a>
              </div>
              <% end %>
            </ul>

          </div>
        </div>
      </div>

    </div>
</div>
